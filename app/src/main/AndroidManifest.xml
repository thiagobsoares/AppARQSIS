<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.projeto.com.br.appservicedesk">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.MenuDeChamados">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controller.NovoChamado" />
        <activity android:name=".controller.ListarChamados" />
        <activity android:name=".controller.ChamadoMensagem" />
        <activity android:name=".controller.FecharChamados" />
        <activity android:name=".controller.FecharChamadosLista" />
        <activity android:name=".controller.ListaTodosChamados" />
        <activity android:name=".controller.ListaPorFila" />
        <activity android:name=".controller.ListaPorStatus"></activity>
        <activity android:name=".controller.DetalheChamadosActivity"></activity>
    </application>
</manifest>